 void dfs(TreeNode *temp,int ans,int &c)
 {
     if(!temp)
        return;
    if(!temp->left && !temp->right)
    {
        c=min(c,ans+1);
        //return;
    }
    if(temp->left)
        dfs(temp->left,ans+1,c);
    if(temp->right)
        dfs(temp->right,ans+1,c);
 }
int Solution::minDepth(TreeNode* A) {
    if(!A)
        return 0;
    int c=INT_MAX;
    dfs(A,0,c);
    return c;
}
